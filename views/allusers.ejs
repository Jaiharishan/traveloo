
<section class="p-5">
    <div class="text-center">
        <div class="h1">All users</div>
        <a href="/dashboard/" class="btn btn-primary text-light">back</a>
    </div>
    
    <div class="container d-flex  flex-wrap justify-content-center">
        
        <% for(let i = 0; i < users.length; i++) { %>
            <% if(users[i].email != self) { %> 
                <div class="card d-flex flex-column align-items-center my-3 mx-sm-5 py-2 px-3" id="user-card">
                    <img src="https://randomuser.me/api/portraits/men/11.jpg" class="img-fluid rounded-circle mt-3 mb-2" width="125px" alt="abc">
                    <div class="card-body">
                        <p class="lead"> <span class="text-primary fw-bold">name:</span> <%= users[i].username %></p>
                        <p class="lead"> <span class="text-primary fw-bold">email:</span>  <%= users[i].email %></p>
                        <p class="lead"> <span class="text-primary fw-bold">gender:</span>  <%= users[i].gender %></p>
                        <p class="lead"> <span class="text-primary fw-bold">nationality:</span>  <%= users[i].country %></p>   
                    </div>
                </div>
            <% } %>
        <% } %>
    </div>
</section>

